@{
    Layout = "_LayoutDashboard";
}

<!-- Começando conteudo !-->
<div class="content-wrapper">

    <div class="container-fluid">

        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="#">Home</a>
            </li>
            <li class="breadcrumb-item">
                <span>Econtrei um aparelho</span>
            </li>
        </ol>

        <div class="row ">
            <div class="col-md-12">
                <p class="text-center mt-md-3 mb-md-3">
                <p class="alert alert-success lead text-center">Aparelho Encontrado!</p>
                </p>
                <form class="container mt-md-5 border p-5" method="post" id="insert_form">
                    <div class="row">

                        <div class="col-md-6">
                            <label class="text-primary text-uppercase" for="emailContato">Nome:</label>
                            <small class="lead">João Alves</small>
                            <br />
                            <label class="text-primary text-uppercase">Telefone:</label>
                            <small class="lead">62 9999-9999</small>
                            <br />
                            <label class="text-primary text-uppercase">IMEI do aparelho Econtrado:</label>
                            <small class="lead">1234567890</small>
                            <br />
                            <label class="texto-login-custom text-primary text-uppercase">Estado:</label>
                            <small class="lead">Goiás</small>
                            <br />
                        </div>

                        <div class="col-md-6">

                            <label class="text-primary text-uppercase" for="emailContato">Endereço:</label>
                            <small class="lead">Rua Elpidio Luiz Brandão</small>
                            <br />
                            <label class="text-primary text-uppercase" for="emailContato">Bairro:</label>
                            <small class="lead">Centro</small>
                            <br />
                            <label class="text-primary text-uppercase" for="emailContato">Número:</label>
                            <small class="lead">S/N</small>
                            <br />
                            <label class="texto-login-custom text-primary text-uppercase">Cidade:</label>
                            <small class="lead">Inhumas</small>
                            <br />
                        </div>
                    </div>
                </form>

                <section id="localizacao" class="div_colorida p-4">
                    <div class="container">
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="page-header"><h1>Localização <small>Veja onde estamos</small></h1></div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12">
                                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d244998.4954411211!2d-49.49838524324433!3d-16.368791559127928!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x935e787b4d91a6f5%3A0x6321d39fddd40271!2sInhumas+-+GO%2C+75400-000!5e0!3m2!1spt-BR!2sbr!4v1526095640051" width="100%" height="520" frameborder="0" style="border:0" allowfullscreen></iframe>
                            </div>
                        </div>
                    </div>
                </section>




            </div>
        </div>

    </div>
    <!-- terminando conteudo !-->
</div>